<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-handler>org.omnifaces.resourcehandler.CDNResourceHandler</resource-handler>
    </application>
    <factory>
        <render-kit-factory>org.omnifaces.renderkit.Html5RenderKitFactory</render-kit-factory>
    </factory>
    <navigation-rule>
        <from-view-id>/admin/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addPackage</from-outcome>
            <to-view-id>/admin/adminAddPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addInsurance</from-outcome>
            <to-view-id>/admin/AdminaddInsurance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changePermission</from-outcome>
            <to-view-id>/admin/AdminChangePermission.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/adminAddPackage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addPackage</from-outcome>
            <to-view-id>/admin/adminAddPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addInsurance</from-outcome>
            <to-view-id>/admin/AdminaddInsurance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changePermission</from-outcome>
            <to-view-id>/admin/AdminChangePermission.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/auth/signin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index_redirect</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/firm/add.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index_redirect</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/device/add.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/client/client.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/client/client.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/device/add.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>addInsurance</from-outcome>
            <to-view-id>/device/insurance/add.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>addPackage</from-outcome>
            <to-view-id>/device/package/add.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>        

    <navigation-rule>
        <from-view-id>/admin/AdminaddInsurance.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addPackage</from-outcome>
            <to-view-id>/admin/adminAddPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addInsurance</from-outcome>
            <to-view-id>/admin/AdminaddInsurance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changePermission</from-outcome>
            <to-view-id>/admin/AdminChangePermission.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/AdminChangePermission.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addInsurance</from-outcome>
            <to-view-id>/admin/AdminaddInsurance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addPackage</from-outcome>
            <to-view-id>/admin/adminAddPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changePermission</from-outcome>
            <to-view-id>/admin/AdminChangePermission.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
        
    <navigation-rule>
        <from-view-id>/connection/connect.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>signin</from-outcome>
            <to-view-id>/auth/signin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>lostpassword</from-outcome>
            <to-view-id>/connection/lostPWD.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>   
    
</faces-config>
